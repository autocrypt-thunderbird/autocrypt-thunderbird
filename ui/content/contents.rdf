<?xml version="1.0"?>
<RDF:RDF xmlns:RDF="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:chrome="http://www.mozilla.org/rdf/chrome#">

  <!-- list all the packages being supplied by this jar -->
  <RDF:Seq about="urn:mozilla:package:root">
    <RDF:li resource="urn:mozilla:package:enigmail"/>
  </RDF:Seq>

  <!-- enigmail package information -->
  <RDF:Description about="urn:mozilla:package:enigmail"
        chrome:displayName="protoZilla"
        chrome:author="xmlterm.org"
        chrome:name="enigmail">
  </RDF:Description>

  <!-- enigmail overlay information -->
  <RDF:Seq about="urn:mozilla:overlays">
    <RDF:li resource="chrome://navigator/content/navigator.xul"/>
  </RDF:Seq>

  <RDF:Seq about="chrome://navigator/content/navigator.xul">
    <RDF:li>chrome://enigmail/content/enigmailOverlay.xul</RDF:li>
  </RDF:Seq>

</RDF:RDF>
